                                    <p style="color:red;font-weight:bold;">
                                        
                                    </p>
                                </div>
                                <div class="upload-img">
                                    <label for="files" class="btn">Selecione a imagem</label>
                                    <input id="files" style="visibility:hidden;" type="file" name="formFile" onchange="readURL(this);">
                                    <button class="btn" type="submit" value="Upload">Carregue a imagem</button>
                                </div>
                               
                            }
                            <!-- Profile picture upload button-->
                            
                        </div>
