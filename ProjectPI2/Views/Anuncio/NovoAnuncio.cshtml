                        <div class="prof-body text-center">
                            <div class='instrucoes'>
                                <h6>Instruções</h6>
                                <p>1º Selecione a imagem (somente JPG)</p>
                                <p>2º Carregue a imagem (ela será enviada para a base de dados)</p>
                                <p>3º Preencha o formulário e então clique em Salvar</p><br>
                            </div>
                            @using (Html.BeginForm("NovoAnuncio", "Anuncio", FormMethod.Post, new { @enctype = "multipart/form-data" }))
